<!doctype html>
<html lang="en">
  <head>
<form class="d-flex" id="search-form">
    <div class="input-group">
        <input
          type="search"
          id="autocomplete"
          class="form-control"
          placeholder="Search"
        >
    </div>
</form>

    <script src="https://kit.fontawesome.com/96d943cb0f.js" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/cloudinary-core/2.12.3/cloudinary-core-shrinkwrap.js" integrity="sha512-KMNn99TvNv6bP4D0H1gNhpVSAf5xds74pMwodUgdMcBUkhDn4Wy0bVXPWs+R/dKJU9HKCQg/9ddC9Ji91OsL6A==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@algolia/algoliasearch-netlify-frontend@1/dist/algoliasearchNetlify.css" />
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@algolia/algoliasearch-netlify-frontend@1/dist/algoliasearchNetlify.js"></script>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@algolia/algoliasearch-netlify-frontend@1/dist/algoliasearchNetlify.css" />
<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@algolia/algoliasearch-netlify-frontend@1/dist/algoliasearchNetlify.js"></script>
<script type="text/javascript">
 // algoliasearchNetlify({
 //   appId: '28SC5WJ2LI',
 //   apiKey: '00d91f5c32cd714cc1b9b0f75effe15e',
//   siteId: '085b06d2-232b-40fb-82a3-bbaa601eeadc',
//    branch: 'main',
  //  selector: 'div#search',
  });
</script>
    <meta charset="utf-8">
    <meta http-equiv="Accept-CH" content="DPR, Viewport-Width, Width">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ title or metadata.title }}</title>
    <meta name="description" content="{{ description or metadata.description }}">

    <!-- Inline and minify CSS - VS Code thinks this is invalid but it's fine -->
    {% set code_css %}
      {% include "_code.css" %}
    {% endset %}
    {% set css %}
      {% include "style.css" %}
    {% endset %}
    <style>
      {{ code_css | cssmin | safe }}
      {{ css | cssmin | safe }}
    </style>

    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path | url }}" type="application/json" title="{{ metadata.title }}">
<link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.css"
/>

  </head>
  <body>
    <header>
      <h1 class="home"><a href="{{ '/' | url }}">{{ metadata.title }}</a></h1>

      {#- Read more about `eleventy-navigation` at https://www.11ty.dev/docs/plugins/navigation/ #}
      <ul class="nav">
      {%- for entry in collections.all | eleventyNavigation %}
        <li class="nav-item{% if entry.url == page.url %} nav-item-active{% endif %}"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
      {%- endfor %}
        <li class="nav-item">  <a href="/feed/feed.xml"> <i class="fa-light fa-square-rss"></i> </a></li> <li class="nav-item"><div id="autocomplete"></div></li>
      </ul>
    </header>

    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
      {{ content | safe }}
    </main>

    {% include 'footer.njk' %}

    <!-- Current page: {{ page.url | url }} -->

    <!-- Before the closing </body> -->
    <script src="https://cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.js"></script>
    <script>
      docsearch({
        // Your Search API Key
        apiKey: '09bb3fdfd614e91a48e6737e613ef9d3',
        // The index populated by the DocSearch scraper
        indexName: 'index',
        // Your Algolia Application ID
        appId: 'WJB3BB1LBY,
        // Replace inputSelector with a CSS selector
        // matching your search input
        inputSelector: '#autocomplete',
        // Set debug to true to inspect the dropdown
        debug: false,
      });
    </script>
  </body>
  <script type="text/javascript">
    var cl = cloudinary.Cloudinary.new({cloud_name: "paulportfolio"});
    // replace 'demo' with your cloud name in the line above
    cl.responsive();
</script>
</html>
